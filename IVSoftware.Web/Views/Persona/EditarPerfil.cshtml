                                    @Html.DisplayNameFor(model => model.ListFormacion[0].Entidad)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.ListFormacion[0].FechaFinalizacion)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.ListFormacion[0].NumeroHoras)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.ListFormacion[0].ArchivoAdjunto)
                                </th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.ListFormacion)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.NombreCurso)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Tema)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.TipoCertificacion.Nombre)
                                    </td>
