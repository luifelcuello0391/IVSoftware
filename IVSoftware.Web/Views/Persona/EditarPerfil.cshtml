                                        </th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.ListConocimientoTecnico)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Analisis)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.TecnicaAnalitica)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Matriz)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Tiempo)
                                            </td>
