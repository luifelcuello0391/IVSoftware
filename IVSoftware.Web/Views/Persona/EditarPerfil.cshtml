                    </div>
                </div>
            </div>
            <div class="collapse" id="collapseConocimientoTecnico" style="margin-top:20px">
                <div class="card card-body">
                    <h6 class="text-success titulo">Conocimiento técnico específico</h6>
                    <div class="row">
                        <div class="col">
                            <table class="table">
                                <thead style="background-color:yellowgreen; color:white">
                                    <tr>
                                        <th>
                                            @Html.DisplayNameFor(model => model.ListConocimientoTecnico[0].Analisis)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.ListConocimientoTecnico[0].TecnicaAnalitica)
