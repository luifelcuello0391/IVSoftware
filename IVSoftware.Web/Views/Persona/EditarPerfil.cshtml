                                    @Html.DisplayNameFor(model => model.ListExperienciaLaboral[0].FechaRetiro)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.ListExperienciaLaboral[0].CargoContrato)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.ListExperienciaLaboral[0].ArchivoAdjunto)
                                </th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.ListExperienciaLaboral)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.NombreEmpresa)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.TipoEmpresa.Nombre)
                                    </td>
                                    <td>
