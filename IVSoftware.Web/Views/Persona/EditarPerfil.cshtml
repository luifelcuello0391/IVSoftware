                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.NumeroHoras)
                                    </td>
                                    <td>
                                        <a href="@Url.Action("DescargarArchivo", "Persona", new { file = item.ArchivoAdjunto })">
                                            <i class="fas fa-file-download" style="font-size:larger; color:gray"></i>
                                        </a>
                                    </td>
                                    <td align="right">
                                        <a asp-controller="Formacion" asp-action="Edit" asp-route-id="@item.Id"><i class="fas fa-edit" style="font-size:larger"></i></a>&nbsp;
                                        <a asp-controller="Formacion" asp-action="Details" asp-route-id="@item.Id"><i class="fas fa-stream" style="font-size:larger; color:green"></i></a>&nbsp;
                                        <a asp-controller="Formacion" asp-action="Delete" asp-route-id="@item.Id"><i class="fas fa-minus-circle" style="font-size:larger; color:red"></i></a>
                                    </td>
