@model ContractType

@{
    ViewData["Title"] = "Detalle";
}

<h5 class="text-primary titulo">Tipo de Contrato - Detalle</h5>
<hr />
<div>
    <div class="row">
        <div class="col-md-8 col-sm-12">
            <div class="form-group">
                <div style="background-color:yellowgreen;padding-top:5px;padding-left:5px;padding-bottom:2px">
                    <label asp-for="Name" class="control-label" style="color:white"></label>
                </div>
                <input asp-for="Name" class="form-control" readonly="readonly" />
            </div>
        </div>
    </div>
</div>
<div>
    <div class="form-group">
        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Editar</a>
        <a asp-action="Index" class="btn btn-primary">Atrás</a>
    </div>
</div>
